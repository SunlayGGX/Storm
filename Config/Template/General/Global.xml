<General>
	<Log>
		<logFolderPath>$[StormTmp]\Logs</logFolderPath>
		<logFileName>log_$[Date]_PID$[PID]</logFileName>
		<logLevel>Debug</logLevel>
		<override>true</override>
		<removeOlderThanDays>7</removeOlderThanDays>
		<fpsWatching>false</fpsWatching>
		<logGraphicDeviceMessage>false</logGraphicDeviceMessage>
	</Log>
</General>